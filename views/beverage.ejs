<% include header.ejs %>
	<div class="header-container">
		<header class="header-height header-image header-img">
			<div class="container">
				<div class="row">
					<div class="col-sm-12 text-center header-text">
						<small>唯有爱与美食不可辜负</small>
						<h3>饮品 美食</h3>
					</div>
				</div>
			</div>
		</header>
	</div>

	<div class="container">
		<div class="divider-whitespace"></div>


		<div class="row">
			<div class="col-md-12 text-center subheader">
				<h3>饮品</h3>
			</div>
		</div>
		<div class="divider-whitespace"></div>
		<div class="row">
			<% for(var i=0; i<data.length; i++) {%>
			   

			   <div class="col-xs-6 col-sm-6 split-image split-text">
					<center>
						<img src=<%= data[i].imageUrl %> class="img-responsive" >
						<h3 class="hh3"><%= data[i].title %></h3>
						<p  class="p"><%= data[i].info %></p>
						<p  class="p">价位：<%= data[i].price %>元</p>
					</center>
				</div>
			<% } %>
		</div>
		<div class="divider-whitespace"></div>

	<footer>
		<div class="container-fluid">
			<div class="row">
				<div class="col-xs-12">
					<p>© 2016 Starbucks Corporation.
						<br>
						All rights reserved. 沪ICP备09020061号
					</p>
				</div>
			</div>
		</div>
	</footer>
	<script src="js/bootstrap.min.js"></script>
	<script>
		$.get("/beverage", function(data) {
            console.log(data);
             
        });

	</script>



</body>
</html>